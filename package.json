{
  "name": "astro-template",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build && npm run format",
    "format": "prettier --write 'dist/**/*.html'",
    "preview": "astro preview",
    "lint": "eslint src/ --ext .astro,.ts",
    "file:build": "astro build && npm run file:format && npm run file:copy && npm run file:list",
    "file:format": "prettier --write 'dist/**/*.html'",
    "file:copy": "node 'scripts/copyDirectory.js'",
    "file:list": "node 'scripts/makeFileList.js'",
    "svgsprite": "node 'scripts/svgSprite.js'",
    "preinstall": "node scripts/initialSetting.js"
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.8",
    "@inquirer/prompts": "^7.0.0",
    "@types/fs-extra": "^11.0.4",
    "astro": "^4.16.3",
    "fs-extra": "^11.2.0",
    "image-size": "^1.1.1",
    "micromodal": "^0.4.10",
    "sharp": "^0.33.5",
    "tree-node-cli": "^1.6.0"
  },
  "devDependencies": {
    "@markuplint/astro-parser": "^4.6.9",
    "@types/micromodal": "^0.3.5",
    "@types/youtube": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "glob": "^11.0.0",
    "markuplint": "^4.10.1",
    "node-html-parser": "^6.1.13",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.79.5",
    "stylelint": "^16.10.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "svg-sprite": "^2.0.4",
    "vituum": "^1.1.1"
  }
}